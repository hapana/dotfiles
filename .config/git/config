[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[pull]
  rebase = true
  default = current
[core]
	excludesfile = ~/.gitignore_global
	autocrlf = input
[push]
  default = current
[rebase]
  autoStash = true
